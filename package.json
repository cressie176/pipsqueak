{
  "name": "pipsqueak",
  "version": "2.4.0",
  "description": "An interval based task runner",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc --report html --reporter lcov --reporter text-summary mocha test",
    "codeclimate": "(test -e .codeclimate && npm run coverage && source .codeclimate) || echo skipping codeclimate",
    "lint": "eslint .",
    "precommit": "npm run lint && npm run test && npm run examples",
    "prepush": "npm run lint && npm run codeclimate",
    "test": "mocha test",
    "examples": "node examples/callback-example && node examples/promise-example && node examples/synchronous-example"
  },
  "keywords": [
    "task",
    "runner",
    "promise",
    "async",
    "sync",
    "await",
    "synchronous",
    "asynchronous",
    "callback",
    "interval",
    "delay"
  ],
  "author": "Stephen Cresswell",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-imperative": "^3.0.0",
    "eslint-plugin-imperative": "^2.1.0",
    "husky": "^1.3.1",
    "mocha": "^6.1.0",
    "nyc": "^13.3.0"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "parse-duration": "^0.1.1",
    "uuid": "^3.3.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cressie176/pipsqueak.git"
  },
  "bugs": {
    "url": "https://github.com/cressie176/pipsqueak/issues"
  },
  "homepage": "https://cressie176.github.io/pipsqueak/"
}
